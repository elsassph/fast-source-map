{
  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/vendor": true,
    "lib/**": true,
    "*.asm": true,
    "*.cfg": true
  },
  "typescript.tsdk": "./node_modules/typescript/lib",
  "json.schemas": [{
    "fileMatch": [
      "/tsconfig.json"
    ],
    "url": "http://json.schemastore.org/tsconfig?update"
  }]
}
