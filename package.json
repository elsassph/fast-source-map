{
  "name": "fast-source-map",
  "version": "0.1.0",
  "description": "fast source mapping decode/encode",
  "main": "index.js",
  "jsnext:main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "do-you-even-bench": "^1.0.2",
    "eslint": "^2.11.1",
