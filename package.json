    "mocha": "^2.5.3",
    "rollup": "^0.25.0",
    "rollup-plugin-babel": "^1.0.0",
    "rollup-plugin-multi-entry": "^1.1.0",
    "sane": "^1.2.0",
    "source-map": "^0.5.0"
  },
  "scripts": {
